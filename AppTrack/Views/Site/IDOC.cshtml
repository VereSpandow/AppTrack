@{
    Layout = "~/Views/Shared/_SiteLayout.cshtml";
    ViewBag.thisPage = "AppTrack";
    ViewBag.PageType = "AppTrack";
}
<div class="row">
    <div class="container nopadding" style="margin-top:0px;">
        <div class="col-md-4  pull-left hidden-xs hidden-sm" style="min-height:450px;">
            @{Html.RenderAction("ContactSubscribe", "Site");}
        </div>
        <div class="col-md-8 pull-right no-offset nopadding noteditable" id="AppTrackSelectImage">
            <div class="no-offset nopadding" style="background-color:#c0d0e6;width:100%;padding:0px;margin:0px;">
                <img class="img-responsive" src="../../Images/AppTrack.png" style="height:auto; width:100%; padding:0px;" alt="Optometry Practice Management" />
            </div>
            <div class="col-md-12 nopadding editable" id="AppTrackMain">                
                @{Html.RenderAction("CMSGetContents", "CMSContents", new { sectionName = "AppTrackMain", status = "LIVE", viewName = "AppTrack" });}
            </div>
        </div>
    </div>
</div>
<div class="row nopadding" style="width:100%;height:25px;">
    &nbsp;
</div>
